<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" fx:id="painelMenuPrincipal" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eFitness.controller.MenuPrincipalController">
   <children>
      <MenuBar AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <menus>
          <Menu mnemonicParsing="false" text="eFitness">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#listarUsuarios" text="Usuário" />
              <MenuItem mnemonicParsing="false" text="Sair" />
            </items>
          </Menu>          
        </menus>
      </MenuBar>
      <ToolBar layoutY="25.0" prefHeight="40.0" prefWidth="200.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="25.0">
        <Button mnemonicParsing="false" onAction="#listarExercicios" text="Exercício" />
         <Button mnemonicParsing="false" onAction="#listarExercicios" text="Aluno" />
      </ToolBar>
   </children>
</AnchorPane>
